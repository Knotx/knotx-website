---
layout: content-layout
---
<div class="container flex blog">
    <div class="row">
      <div class="col-sm-12">
        <article>
          <%- @partial('contributor-meta', {author: @document.author, date: @document.date}) %> 
          <section class="post">
            <%- @content %>
          </section>
        </article>                        
      </div>        
    </div>
</div>        
