<header class="meta">
  <a class="meta-item" href="<%= @getMemberGithub(@getMember(@author)) %>"><%= @getMember(@author).name %></a>
  <time class="meta-item date"
        datetime="<%= @formatDt(@date) %>"><%= @formatDate(@date) %>
  </time>
  <span class="meta-item">Knot.x version: <%= @knotxVersion %></span>
</header>
