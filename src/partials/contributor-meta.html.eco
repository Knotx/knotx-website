<div class="meta">
  <a class="meta-item" href="<%= @getMemberGithub(@getMember(@author)) %>"><%= @getMember(@author).name %></a>
  <time class="meta-item date"
        datetime="<%= @formatDt(@date) %>"><%= @formatDate(@date) %>
  </time>
  <% if @knotxVersion: %>
    <span class="meta-item"><a class="meta-item" href="<%= @getGithubReleaseTag(@knotxVersion) %>"><%= @printKnotxVersion(@knotxVersion) %></a></span>
  <% end %>
</div>
